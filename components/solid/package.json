{
  "name": "@zui/solid",
  "type": "module",
  "private": true,
  "scripts": {
    "prepare": "panda codegen --clean --silent",
    "dev": "bun run storybook",
    "lint": "biome lint ./src",
    "storybook": "storybook dev -p 6006",
    "typecheck": "tsc",
    "registry:prepare": "scripts registry prepare --name @zui/solid",
    "registry:build": "scripts registry build solid",
    "typedoc": "scripts typedoc --out ../../website/public/types/solid"
  },
  "dependencies": {
    "@ark-ui/solid": "5.30.0",
    "@tanstack/solid-virtual": "3.13.18",
    "lucide-solid": "0.563.0"
  },
  "devDependencies": {
    "@pandacss/dev": "1.8.2",
    "@pandacss/preset-base": "1.8.2",
    "@pandacss/preset-panda": "1.8.2",
    "@zui/preset": "workspace:*",
    "@zui/scripts": "workspace:*",
    "@storybook/addon-a11y": "10.2.0",
    "@storybook/addon-themes": "10.2.0",
    "lorem-ipsum": "2.0.8",
    "solid-js": "1.9.11",
    "storybook": "10.2.0",
    "storybook-solidjs-vite": "10.0.9",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vite-plugin-solid": "2.11.10",
    "vite-tsconfig-paths": "6.0.5"
  },
  "peerDependencies": {
    "solid-js": ">=1.9.0"
  }
}
