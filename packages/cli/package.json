{
  "name": "@zalify-ui/cli",
  "version": "1.0.7",
  "description": "CLI tool for Zalify UI components",
  "type": "module",
  "bin": {
    "zalify-ui": "dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": {
    "name": "zalifyui",
    "url": "https://x.com/zalifyui"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zalify/zalify-ui.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/zalify/zalify-ui/issues"
  },
  "homepage": "https://ui.zalify.com",
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "components",
    "ui",
    "panda",
    "ark",
    "react",
    "solid",
    "zalify-ui"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "start": "bun run ./src/index.ts",
    "lint": "biome lint ./src",
    "test": "bun test ./src",
    "build": "tsup",
    "test:local": "node ./scripts/local-test.mjs",
    "release-it": "release-it"
  },
  "dependencies": {
    "@better-fetch/fetch": "1.1.21",
    "@clack/prompts": "1.0.1",
    "commander": "14.0.3",
    "cosmiconfig": "9.0.0",
    "effect": "3.19.19",
    "fs-extra": "11.3.3",
    "magicast": "0.5.2",
    "package-directory": "8.2.0",
    "picocolors": "1.1.1",
    "ts-morph": "27.0.2",
    "tsconfck": "3.1.6",
    "tsconfig-paths": "4.2.0",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@pandacss/dev": "1.8.2",
    "@release-it/keep-a-changelog": "7.0.1",
    "@types/fs-extra": "11.0.4",
    "@types/node": "25.3.0",
    "release-it": "19.2.4",
    "tsup": "8.5.1",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
